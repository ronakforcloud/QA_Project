package tests;

public class StringTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s1 = "Ronak";
		String s2 = "Patel";
		String s3 = "Patel";
		String s4 = new String("Patel");
		if (s1==s2){
			System.out.println("pass");
		}else {
			System.out.println("fail");
		}
		if (s3==s4) {
			System.out.println("pass");
		}else {
			System.out.println("fail");
		}
		if (s3.equals(s4)){
			System.out.println("Pass");
		}else {
			System.out.println("fail");
		}
		if (s2==s3) {
			System.out.println("pass");
		}else {
			System.out.println("fail");
		}
	}

}
